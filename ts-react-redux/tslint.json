{
    "defaultSeverity": "error",
    "extends": ["tslint:latest", "tslint-react"],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "semicolon": [true, "never"],
      "interface-name": [true, "always-prefix"],
      "object-literal-sort-keys": false,
      "no-console": false
    },
    "rulesDirectory": []
}